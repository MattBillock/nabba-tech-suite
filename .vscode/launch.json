    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/bandmanager/manage.py",
            "args": [
                "runserver"
            ],
            "django": true
        },
        {
            "name": "Django MGMT Command",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/bandmanager/manage.py",
            "args": [
                "generate_contest_json",
                "2"
            ],
            "django": true
        }
    ]
}